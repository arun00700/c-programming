#include<stdio.h>
     int main()
     {
     int num;
     printf("Insert a number: ");
     scanf("%d”,&num);
     //Condition to check if the number is negative or positive
     int f=0;
     for(int i=2;i<num;i++){
         
         if(num%i==0){
             f++;
         }
     }
     if (f==0)
         printf("%d is prime number",num);
     else
         printf("%d is not prime number",num);
       
     return 0;

 }
