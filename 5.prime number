#include<stdio.h>
     int main()
     {
     int num;
     printf("Insert a number: ");
     scanf("%dâ€,&num);
     //Condition to check if the number is negative or positive
     for (int j=5;j<=num;j++){
        int f=0;
        for(int i=2;i<j;i++){
         if(j%i==0)
             f++;
         }
        if (f==0)
          printf("%d is prime number\n",j);
        else
          printf("%d is not prime number\n",j);
     }
     return 0;

 }
