// Online C compiler to run C program online
#include<stdio.h>
int main()
{ 

  int no, digit,sum1, sum = 0;

    printf ("Insert a number \n"); 
    scanf ("%d", &no); 
    sum1=no;
    while  (no > 0) 
    { 
        digit = no % 10; 
        printf("%d\n",digit );
        sum = sum * 10 +digit;
        printf("%d\n",sum);
        no /= 10;
        printf("%d\n",no);
    } 
    if (sum1==sum)
           printf(" the numbers are palindrome %d ", sum); 
  return 0;
} 
